package fr.utt.lo02.carte;

import java.util.ArrayList;

import fr.utt.lo02.partie.Partie;

public class TourJoueur {

	private ArrayList<int> ListeCarte;
	private Partie partie;
	
	public TourJoueur(Partie partie) {
		this.ListeCarte = new ArrayList<Carte>();
		this.partie = partie;
	}

	public ArrayList<Carte> getListeCarte() {
		return ListeCarte;
	}
	
	public void addCarteListe(int i){
		this.ListeCarte.add(i);
	}
	
	public void setListeCarte(ArrayList<Carte> listeCarte) {
		ListeCarte = listeCarte;
	}
	
	
}
